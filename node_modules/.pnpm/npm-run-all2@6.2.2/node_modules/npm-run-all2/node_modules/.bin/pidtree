#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules/pidtree/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules/pidtree/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/npm-run-all2@6.2.2/node_modules/npm-run-all2/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/npm-run-all2@6.2.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules/pidtree/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules/pidtree/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pidtree@0.6.0/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/npm-run-all2@6.2.2/node_modules/npm-run-all2/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/npm-run-all2@6.2.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pidtree@0.6.0/node_modules/pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../../../../../pidtree@0.6.0/node_modules/pidtree/bin/pidtree.js" "$@"
fi
