#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pinia@2.2.2_typescript@5.4.5+vue@3.4.38/node_modules/pinia/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pinia@2.2.2_typescript@5.4.5+vue@3.4.38/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pinia@2.2.2_typescript@5.4.5+vue@3.4.38/node_modules/pinia/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/pinia@2.2.2_typescript@5.4.5+vue@3.4.38/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.4.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.4.5/node_modules/typescript/bin/tsserver" "$@"
fi
