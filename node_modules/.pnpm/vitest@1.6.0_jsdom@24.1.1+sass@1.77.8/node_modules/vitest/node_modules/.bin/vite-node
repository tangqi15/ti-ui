#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite-node@1.6.0_sass@1.77.8/node_modules/vite-node/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite-node@1.6.0_sass@1.77.8/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vitest@1.6.0_jsdom@24.1.1+sass@1.77.8/node_modules/vitest/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vitest@1.6.0_jsdom@24.1.1+sass@1.77.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite-node@1.6.0_sass@1.77.8/node_modules/vite-node/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite-node@1.6.0_sass@1.77.8/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vitest@1.6.0_jsdom@24.1.1+sass@1.77.8/node_modules/vitest/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vitest@1.6.0_jsdom@24.1.1+sass@1.77.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@1.6.0_sass@1.77.8/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@1.6.0_sass@1.77.8/node_modules/vite-node/vite-node.mjs" "$@"
fi
