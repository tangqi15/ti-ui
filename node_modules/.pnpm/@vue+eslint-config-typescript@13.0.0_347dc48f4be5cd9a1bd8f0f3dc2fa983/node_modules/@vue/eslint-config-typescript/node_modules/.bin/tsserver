#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules/@vue/eslint-config-typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules/@vue/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules/@vue/eslint-config-typescript/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules/@vue/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/@vue+eslint-config-typescript@13.0.0_347dc48f4be5cd9a1bd8f0f3dc2fa983/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@5.4.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@5.4.5/node_modules/typescript/bin/tsserver" "$@"
fi
