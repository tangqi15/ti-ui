#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules/prettier/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules/prettier/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/eslint-plugin-prettier@5.2.1_4300439fcc0c5ddc5a8b7cb962c16110/node_modules/eslint-plugin-prettier/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/eslint-plugin-prettier@5.2.1_4300439fcc0c5ddc5a8b7cb962c16110/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules/prettier/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules/prettier/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/prettier@3.3.3/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/eslint-plugin-prettier@5.2.1_4300439fcc0c5ddc5a8b7cb962c16110/node_modules/eslint-plugin-prettier/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/eslint-plugin-prettier@5.2.1_4300439fcc0c5ddc5a8b7cb962c16110/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@3.3.3/node_modules/prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../../../../../prettier@3.3.3/node_modules/prettier/bin/prettier.cjs" "$@"
fi
