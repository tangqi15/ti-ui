#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules/vite/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules/vite/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vite/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules/vite/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules/vite/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vite@5.4.1_@types+node@20.16.1/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vite/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
