#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules/vue-tsc/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules/vue-tsc/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vue-tsc/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vue-tsc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules/vue-tsc/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules/vue-tsc/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/vue-tsc@2.0.29_typescript@5.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vue-tsc/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/vue-tsc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
fi
