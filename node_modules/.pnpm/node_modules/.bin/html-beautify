#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/js/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/js/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/js-beautify@1.15.1/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/js/bin/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/js/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/js-beautify/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-beautify/js/bin/html-beautify.js" "$@"
else
  exec node  "$basedir/../js-beautify/js/bin/html-beautify.js" "$@"
fi
