#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/esm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/dist/esm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/dist/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/esm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules/glob/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/glob@10.4.5/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules:/Users/workSpace/workSpace/node_modules:/Users/workSpace/node_modules:/Users/node_modules:/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/dist/esm/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/dist/node_modules:/Users/workSpace/workSpace/ti-ui/node_modules/.pnpm/node_modules/glob/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../glob/dist/esm/bin.mjs" "$@"
fi
